{
    "manifest_version": 3,
    "name": "Simple_Pagina",
    "description": "Pagina Sin estilos",
    "version": "1",
    "icons": {
      "128": "icono.png"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://*/*"
    ]
  }